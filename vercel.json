{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/music",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate=59"
      }
    },
    {
      "src": "/blog",
      "dest": "/index.html"
    },
    {
      "src": "/projects",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/index.html"
    },
    {
      "src": "/identity",
      "dest": "/index.html"
    },
    {
      "src": "/ventures",
      "dest": "/index.html"
    },
    {
      "src": "/connect",
      "dest": "/index.html"
    },
    {
      "src": "/message",
      "dest": "/index.html"
    },
    {
      "src": "/chill",
      "dest": "/index.html"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}